{
	"name": "firelordjs",
	"version": "2.3.0",
	"description": "ðŸ”¥ Extremely High Precision Typescript Wrapper for Firestore Web, Providing Unparalleled Type Safe and Dev Experience",
	"main": "dist/cjs/index.js",
	"types": "dist/cjs/index.d.ts",
	"scripts": {
		"reinstall": "rm -rf node_modules && rm -rf package-lock.json && npm i && npx husky install",
		"tsc": "tsc",
		"lint": "eslint . --ext .ts --fix",
		"pretty": "pretty-quick --stage --branch main",
		"test": "npm run kill && firebase emulators:exec --only firestore \"rm -rf coverage && jest\" && npm run kill",
		"rollup": "rm -rf dist && rollup -c",
		"build": "rm -rf dist && tsc -p tsconfig.esm.prod.json && tsc -p tsconfig.prod.json",
		"link": "npm unlink firelordjs && npm link && npm link firelordjs",
		"babel": "babel-node -x .ts -- src/fieldPath/documentId.ts",
		"jest": "jest src/operations/getCountFromServer.test.ts --collectCoverage=false",
		"commit": "git commit --no-verify",
		"kill": "kill-port 8080",
		"dev": "---------------------dev------------------------",
		"d-link": "npm run build && npm run link",
		"d-test": "tsc && npm test",
		"d-b-tsc": "npm run d-link && npm run b-uninstall && npm run b-tsc",
		"d-b-test": "npm run d-b-tsc && npm run b-test",
		"d-p-tsc": "npm run p-reinstall && npm run p-tsc",
		"d-p-test": "npm run d-p-tsc && npm run p-test",
		"d-pub": "npm run build && npm publish --tag beta",
		"built": "-------------test built file-----------------------",
		"b-tsc": "tsc -p tsconfig.doc.json",
		"b-test": "firebase emulators:exec --only firestore \"jest -c=jest.config.doc.js\"",
		"b-uninstall": "npm --prefix codeForDoc run uninstall",
		"published": "-------------test published file-----------------------",
		"p-reinstall": "npm --prefix codeForDoc run reinstall && babel-node -x .ts scripts/installLatest.ts",
		"p-test": "npm --prefix codeForDoc test",
		"p-tsc": "npm --prefix codeForDoc run tsc"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,md,json,yml}": [
			"eslint --fix",
			"npm run pretty"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tylim88/firelordjs.git"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/tylim88/firelordjs/issues"
	},
	"homepage": "https://firelordjs.com",
	"keywords": [
		"firebase",
		"firestore",
		"wrapper",
		"typescript",
		"web",
		"scalable",
		"type-safe",
		"V9"
	],
	"publishConfig": {
		"export": {
			".": {
				"import": {
					"types": "./dist/esm/index.d.ts",
					"default": "./dist/esm/index.js"
				},
				"require": {
					"types": "./dist/cjs/index.d.ts",
					"default": "./dist/cjs/index.js"
				}
			}
		}
	},
	"devDependencies": {
		"@babel/node": "^7.18.9",
		"@babel/preset-env": "^7.17.10",
		"@babel/preset-typescript": "^7.16.7",
		"@firebase/rules-unit-testing": "^2.0.2",
		"@rollup/plugin-typescript": "^8.5.0",
		"@types/jest": "^27.5.0",
		"@types/lodash": "^4.14.182",
		"@types/shelljs": "^0.8.11",
		"@typescript-eslint/eslint-plugin": "^5.36.2",
		"@typescript-eslint/parser": "^5.36.2",
		"betwin": "^1.1.3",
		"dotenv": "^16.0.0",
		"eslint": "^8.31.0",
		"eslint-cli": "^1.1.1",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-typescript": "^2.7.1",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-markdown": "^3.0.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-yml": "^1.4.0",
		"firebase": "^9.17.1",
		"husky": "^8.0.1",
		"jest": "^28.1.0",
		"kill-port": "^2.0.1",
		"lint-staged": "^13.1.0",
		"lodash": "^4.17.21",
		"pick-random": "^3.0.0",
		"prettier": "^2.6.2",
		"pretty-quick": "^3.1.3",
		"rollup": "^2.79.0",
		"rollup-plugin-terser": "^7.0.2",
		"shelljs": "^0.8.5",
		"tsdoc": "^0.0.4",
		"typescript": "^4.8.2"
	},
	"peerDependencies": {
		"firebase": "^9.17.1"
	}
}
